@model Vidly.ViewModels.RandomMoviewViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-hover table-primary" style="width: 100%">
    <tr>
        <th>Firstname</th>
    </tr>
    <td>
        @foreach (var customer in Model.Customers)
        {
            <li>@Html.ActionLink(customer.Name, "Details", "Customers")</li>
        }
    </td>
    
</table>



